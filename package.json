{
  "name": "reactjs-base-typescript",
  "version": "0.1.0",
  "author": "daonghia2404@gmail.com",
  "private": true,
  "scripts": {
    "start": "env-cmd -f .env react-app-rewired --openssl-legacy-provider start",
    "build": "env-cmd -f .env react-app-rewired --openssl-legacy-provider build",
    "build:netlify": "CI=false env-cmd -f .env.production react-app-rewired --openssl-legacy-provider build",
    "build:dev": "env-cmd -f .env.dev react-app-rewired --openssl-legacy-provider build",
    "build:production": "env-cmd -f .env.production react-app-rewired --openssl-legacy-provider build",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "rm-nodemodules": "find . -name 'node_modules' -type d -prune -exec rm -rf '{}' +",
    "rm-packagelocks": "find . -name 'package-lock.json' -type f -prune -exec rm -rf '{}' +",
    "rm-yarnlocks": "find . -name 'yarn.lock' -type f -prune -exec rm -rf '{}' +",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "lint": "eslint --fix src/**/*.{ts,tsx}",
    "lint:staged": "lint-staged",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -o storybook-public",
    "create-service": "node ./tools/create-service.js",
    "create-component": "node ./tools/create-component.js",
    "delete-service": "node ./tools/delete-service.js",
    "delete-component": "node ./tools/delete-component.js"
  },
  "dependencies": {
    "@reach/router": "^1.3.4",
    "@storybook/client-api": "^6.3.7",
    "@storybook/react": "^6.2.9",
    "@types/lodash": "^4.14.186",
    "@types/react-slick": "^0.23.8",
    "@types/uuid": "^8.3.3",
    "antd": "^4.19.2",
    "axios": "^0.21.1",
    "babel-loader": "8.1.0",
    "classnames": "^2.3.1",
    "deox": "^3.3.1",
    "formik": "^2.2.6",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "qs": "^6.10.1",
    "react": "^17.0.2",
    "react-cookies": "^0.1.1",
    "react-dom": "^17.0.2",
    "react-google-map-picker": "^1.2.3",
    "react-redux": "^7.2.3",
    "react-responsive": "^9.0.2",
    "react-scripts": "4.0.3",
    "react-slick": "^0.29.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.9",
    "redux-saga": "^1.1.3",
    "resolve-url-loader": "^4.0.0",
    "sass": "^1.56.0",
    "slick-carousel": "^1.8.1",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^4.1.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/node-logger": "^6.2.9",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/preset-scss": "^1.0.3",
    "@types/node": "^12.0.0",
    "@types/reach__router": "^1.3.7",
    "@types/react": "^17.0.0",
    "@types/react-cookies": "^0.1.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.16",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "css-loader": "5.2.7",
    "customize-cra": "^1.0.0",
    "env-cmd": "^10.1.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.5",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "4.3.8",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "react-app-rewired": "^2.1.8",
    "sass-loader": "10.1.1",
    "style-loader": "2.0.0"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "yarn lint",
      "yarn format"
    ],
    "**/*.{tsx,ts}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": "react-app",
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "resolutions": {
    "//": "See https://github.com/facebook/create-react-app/issues/11773",
    "react-error-overlay": "6.0.9"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ]
}
